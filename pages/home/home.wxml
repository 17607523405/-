<!--pages/home/home.wxml-->
<!-- 轮播图展示 -->
<w-swiper list="{{bannes}}"></w-swiper>
<!-- 推荐信息的展示 -->
<w-recommend recommends="{{recommends}}" bindloadimage="bindloadimage"/>
<!-- 本周流行 -->
<view class="popular">
  <image src="../../assets/images/home/recommend_bg.jpg"></image>
</view>
<w-tabbar title="{{titles}}" bindcounter="counter" id="tabbar" class="{{isfixed ?'tabbar-fiexd' : ''}}"></w-tabbar>
<w-tabbar title="{{titles}}" bindcounter="counter" wx:if="{{isfixed}}" />
<w-good goods="{{goods[currenttype].list}}"></w-good>
<w-back-top wx:if="{{isShow}}"></w-back-top>